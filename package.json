{
  "name": "peersox",
  "version": "0.3.0",
  "description": "Simple, high performance client and server for P2P WebRTC and WebSocket data connections.",
  "author": "Jan Hug <me@dulnan.net>",
  "license": "MIT",
  "keywords": [
    "webrtc",
    "websocket",
    "p2p",
    "data channel",
    "data",
    "peer",
    "performance",
    "peer",
    "pairing"
  ],
  "files": [
    "lib",
    "src",
    "LICENSE",
    "README.md"
  ],
  "main": "lib/peersox.server.js",
  "browser": "lib/peersox.client.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dulnan/peersox.git"
  },
  "scripts": {
    "build": "webpack --env build --mode production",
    "docs": "npm run docs:clean && npm run docs:jsdoc && npm run docs:assets",
    "docs:clean": "rm -rf ./docs",
    "docs:jsdoc": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
    "docs:assets": "cp -r misc docs/misc",
    "dev": "webpack --progress --colors --watch --env dev --mode development",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "eventemitter3": "^3.1.2",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "isomorphic-ws": "^4.0.1",
    "redis-mock": "^0.44.0",
    "simple-peer": "git+https://github.com/feross/simple-peer.git#8bf4f675a99d0aa3fc243a88e2f6e097742df3b5",
    "string-hash": "^1.1.3",
    "universal-cookie": "^4.0.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jsdoc": "^3.6.2",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "minami": "^1.2.3",
    "mocha": "^6.1.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^13.2.4"
  }
}
